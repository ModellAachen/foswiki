fixLineEnds : true,

ignore : 
[
	'_dev',
	'_docs/api',
	'_samples',
	'_source/adapters'
],

copy :
[
	{
		source : '_source/lang',
		target : 'lang',
		minify : true
	}
],

packages :
[
	'ckeditor.pack'
],

documentation :
{
	config: '_dev/docs_build/docs_build.conf',
	target: '_docs/api'
},

samples :
{
	source : '_samples',
	template : '_samples/samples.html'
},

skins :
{
	source : '_source/skins',
	target : 'skins'
}
