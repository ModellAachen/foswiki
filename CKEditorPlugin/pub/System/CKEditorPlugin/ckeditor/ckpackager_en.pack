/*
 * CKPackager - Sample Package file
 */

header :
	'/*'												+ '\n' +
	' * This file has been compressed with CKPackager'	+ '\n' +
	' */'												+ '\n' +
	'\n',

constants :
	{
		'BASIC_COLOR_RED'	: 1,
		'BASIC_COLOR_BLUE'	: 2,
		'BASIC_COLOR_WHITE'	: 3,
		'MyObject.CONST'	: 1
	},

packages :
	[
		{
			output : 'lang/en.js',

			renameGlobals : false,
			compactJavascript : true,
			files :
				[
					'_source/lang/en.js'
				]
		}
	]
